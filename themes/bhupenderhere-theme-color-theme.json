{
  "name": "bhupenderhere-theme",
  "type": "dark",
  "colors": {
    // Editor
    "editorCursor.foreground": "#ffffff",
    "editorGroup.emptyBackground": "#101732ab",
    "editor.lineHighlightBorder": "#ffffff11",
    "editor.background": "#10173281",
    "editor.selectionHighlightBorder": "#24e41286",
    "editorIndentGuide.background": "#00ffffad",
    "editorLineNumber.activeForeground": "#ffffff",
    "editor.lineHighlightBackground": "#184053c5",
    "editorIndentGuide.activeBackground": "#fc630bfa",
    "editorSuggestWidget.background": "#101732",
    "editorSuggestWidget.foreground": "#8F93A2",
    "editorSuggestWidget.highlightForeground": "#80CBC4",
    "editorSuggestWidget.border": "#FFFFFF10",
    "editorWidget.background": "#101732",
    "editorWidget.resizeBorder": "#80CBC4",
    "editorMarkerNavigation.background": "#8F93A205",

    // Peek Window
    "peekViewEditorGutter.background": "#101732",
    "peekViewResult.background": "#101732",
    "peekViewEditor.background": "#101732",
    "peekViewTitle.background": "#101732",
    "peekViewEditor.matchHighlightBackground": "#13a5b6",

    // Top Bar
    "titleBar.activeBackground": "#101732",
    "titleBar.activeForeground": "#ffffffb4",
    "titleBar.inactiveBackground": "#101732",

    // Left Bar
    "activityBarBadge.background": "#F26F77",
    "activityBar.background": "#101732",
    "activityBar.activeBackground": "#8fb4ec2a",
    "activityBar.inactiveForeground": "#13a5b6",
    "activityBar.activeBorder": "#F4A24C",
    "activityBarBadge.foreground": "#ffffff",
    "activityBar.foreground": "#01ff01",

    // Left Bar {Child Bar}
    "sideBarTitle.foreground": "#F4A24C",
    "sideBarSectionHeader.foreground": "#10bccf",
    "sideBarSectionHeader.background": "#ffffff1e",
    "sideBar.background": "#090b31d2",

    // Left Bar {Child Bar [Lists]}
    "list.activeSelectionBackground": "#ffffff1c",
    "list.errorForeground": "#F26F77",
    "list.hoverForeground": "#ffffff",
    "list.hoverBackground": "#ffffff1e",

    // Bottom Bar
    "statusBar.background": "#101732",
    "statusBar.foreground": "#fff",
    "statusBar.noFolderBackground": "#101732",
    "statusBar.noFolderForeground": "#ffffff",
    "statusBarItem.hoverBackground": "#21c75869",

    // Dropdowns
    "dropdown.background": "#101732",
    "dropdown.border": "#ffffff59",
    "dropdown.foreground": "#ffffff",

    // Debug Toolbar
    "debugToolBar.background": "#171b3f",
    "debugToolBar.border": "#ffffff42",

    // Bracket Pair Indicator
    "editorBracketMatch.background": "#ffffff56",

    // Icons Background
    "icon.foreground": "#48ec71",

    // For Every Links
    "textLink.foreground": "#f83f96",

    // Terminal Text Colors
    "terminal.ansiGreen": "#A6D050",
    "terminal.ansiYellow": "#F4A24C",
    "terminal.ansiMagenta": "#F26F77",
    "terminal.ansiCyan": "#13a5b6",
    "terminal.ansiBrightWhite": "#fdf6e3",

    // Tabs
    "tab.activeForeground": "#ffffff",
    "tab.activeBorder": "#F26F77",
    "tab.inactiveForeground": "#93A1A1",
    "tab.inactiveBackground": "#101732",
    "tab.hoverForeground": "#ffffff",
    "tab.border": "#1fcdfd00",
    "editorGroupHeader.tabsBackground": "#101732",
    "tab.lastPinnedBorder": "#2aa19700",

    // Terminal Pannel
    "panel.border": "#535370",
    "panel.background": "#101732",
    "panelTitle.activeBorder": "#f26f77",
    "panelTitle.activeForeground": "#ffffff",

    // Input Bars
    "focusBorder": "#ffffff99",
    "input.border": "#ffffff85",
    "input.foreground": "#ffffff",
    "input.background": "#10173279",
    "input.placeholderForeground": "#93a1a1de",
    "inputOption.activeBorder": "#ffffff",
    "inputValidation.infoBorder": "#363b5f",
    "inputValidation.infoBackground": "#052730",
    "inputValidation.warningBackground": "#5d5938",
    "inputValidation.warningBorder": "#9d8a5e",
    "inputValidation.errorBackground": "#101732",
    "inputValidation.errorBorder": "#F26F77",
    "inputValidation.errorForeground": "#ff0000",

    // Git Decoration
    "gitDecoration.conflictingResourceForeground": "#ff62b1",
    "gitDecoration.untrackedResourceForeground": "#4bf53c",
    "gitDecoration.ignoredResourceForeground": "#31cfff",
    "gitDecoration.modifiedResourceForeground": "#d2a0fc",
    "gitDecoration.addedResourceForeground": "#fc4384"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "string.comment"],
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#ffffff75"
      }
    },
    {
      "name": "Key",
      "scope": [
        "meta.property-name.css",
        "meta.property-list.css",
        "source.css"
      ],
      "settings": {
        "foreground": "#ff2ba7"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.template"],
      "settings": {
        // "fontStyle": "italic",
        "foreground": "#84eb6f"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#cc60e7"
      }
    },
    {
      "name": "Brackets",
      "scope": "punctuation.bracket.round.java",
      "settings": {
        "foreground": "#b6a613"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#29ff5e"
      }
    },
    {
      "name": "Agrument Inside",
      "scope": ["meta.method.body.java"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Language Variable",
      "scope": "variable.language",
      "settings": {
        "foreground": "#F34A3A"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.operator.logical",
        "keyword.operator.constructor"
      ],
      "settings": {
        "foreground": "#f4a24c"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword.other.unit.px.css"],
      "settings": {
        "foreground": "#c4e728"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#ff0000"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#F26F77"
      }
    },
    {
      "name": "Types",
      "scope": "storage.type",
      "settings": {
        "foreground": "#1dd670"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.module",
        "entity.name.type",
        "storage.identifier",
        "support.class"
      ],
      "settings": {
        "foreground": "#A6D050"
      }
    },
    {
      "name": "Variable Object",
      "scope": [
        "variable.other.object",
        "variable.other.constant",
        "variable.other.global",
        "variable.other.readwrite.class",
        "variable.other.readwrite.instance",
        "variable.other.readwrite.batchfile",
        "variable.readwrite",
        "variable.readwrite.other.block"
      ],
      "settings": {
        "foreground": "#00ccff"
      }
    },
    {
      "name": "Other variable",
      "scope": [
        "variable.other",
        "variable.other.property",
        "variable.other.block"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Inherited Class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#ffbf00"
      }
    },
    {
      "name": "Package Import",
      "scope": ["storage.modifier.import", "storage.modifier.package"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#fa01a7"
      }
    },
    {
      "name": "Function argument",
      "scope": [
        "variable.parameter",
        "entity.name.variable.parameter",
        "parameter.variable"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00eeff"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function-call",
      "settings": {
        "foreground": "#abb2bf"
      }
    },
    {
      "name": "Builtin Functions",
      "scope": ["function.support.builtin", "function.support.core"],
      "settings": {
        "foreground": "#ffbf00"
      }
    },
    {
      "name": "Tag Name",
      "scope": ["entity.name.tag", "entity.name.tag.class.js"],
      "settings": {
        "foreground": "#F26F77"
      }
    },
    {
      "name": "Angle Brackets",
      "scope": [
        "punctuation.definition.tag.begin.html",
        "meta.tag.structure.body.start.html",
        "punctuation.definition.tag.end.html",
        "meta.tag.structure.html.start.html"
      ],
      "settings": {
        "foreground": "#9b9292"
      }
    },
    {
      "name": "Letters",
      "scope": ["text.html.derivative"],
      "settings": {
        "foreground": "#e6e1e1"
      }
    },
    {
      "name": "Tag Class and ID",
      "scope": ["entity.name.tag.class", "entity.name.tag.id"],
      "settings": {
        "foreground": "#04ff26"
      }
    },
    {
      "name": "Tag Attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#13a5b6"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.variable"],
      "settings": {
        "foreground": "#ac4e4e"
      }
    },
    {
      "name": "StyleSheet Property name",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.scss",
        "support.type.property-name.less",
        "support.type.property-name.sass"
      ],
      "settings": {
        "foreground": "#A6D050"
      }
    },
    {
      "name": "CSS: Pseudo Attribute Names",
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-class.scss",
        "entity.other.attribute-name.pseudo-class.less",
        "entity.other.attribute-name.pseudo-class.sass",
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.pseudo-element.scss",
        "entity.other.attribute-name.pseudo-element.less",
        "entity.other.attribute-name.pseudo-element.sass"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#e732ab"
      }
    },
    {
      "name": "StyleSheet Property value",
      "scope": [
        "support.constant.css",
        "support.constant.scss",
        "support.constant.less",
        "support.constant.sass"
      ],
      "settings": {
        "foreground": "#17c01f"
      }
    },
    {
      "name": "StyleSheet Variable",
      "scope": [
        "variable.css",
        "variable.scss",
        "variable.less",
        "variable.sass"
      ],
      "settings": {
        "foreground": "#e9ec0c"
      }
    },
    {
      "name": "StyleSheet Variable String",
      "scope": [
        "variable.css.string",
        "variable.scss.string",
        "variable.less.string",
        "variable.sass.string"
      ],
      "settings": {
        "foreground": "#b5d99c"
      }
    },
    {
      "name": "StyleSheet Unit",
      "scope": ["unit.css", "unit.scss", "unit.less", "unit.sass"],
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "name": "StyleSheet Function",
      "scope": [
        "function.css",
        "function.scss",
        "function.less",
        "function.sass"
      ],
      "settings": {
        "foreground": "#04e6ff"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "JSON String",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#4a84da"
      }
    },
    {
      "name": "Link",
      "scope": "string.detected-link",
      "settings": {
        "foreground": "#61afef"
      }
    },
    {
      "name": "diff.header",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#75715E"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#e65f5c"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#ffbf00"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#b5d99c"
      }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "#04e6ffA0"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#b5d99c"
      }
    },
    {
      "name": "punctuation",
      "scope": ["entity.other.attribute-name.class.css"],
      "settings": {
        "foreground": "#c811ff"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": "markup.italic, markup.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Emphasis Punctuation",
      "scope": "punctuation.definition.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown: Link",
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#61afef"
      }
    },
    {
      "name": "Markup: Quote",
      "scope": "markup.quote.markdown",
      "settings": {
        "foreground": "#ffbf00"
      }
    },

    {
      "name": "Markup: Raw",
      "scope": ["markup.raw.inline.markdown", "markup.raw.block.markdown"],
      "settings": {
        "foreground": "#04e6ff"
      }
    }
  ]
}
